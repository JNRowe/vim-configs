{
    "ftdetect/*.vim": {
        "template": [
            "augroup jnrowe_ftdetect",
            "    autocmd BufRead,BufNewFile FIXME setfiletype FALLBACK FIXME",
            "augroup END"
        ],
        "type": "fconfig"
    },
    "localcfg/plugin_*.vim": {
        "template": [
            "scriptencoding utf-8",
            "",
            "nnoremap [{capitalize}] <Nop>",
            "nmap <LocalLeader>FIXME [{capitalize}]"
        ],
        "type": "xconfig"
    },
    "localcfg/*.vim": {
        "type": "lconfig"
    },
    "*.rst": {
        "alternate": "{}.html",
        "type": "doc"
    },
    "dein.vim": {"type": "dein"},
    "vimrc": {"type": "rc"},
    "vimrc.d/*.vim": {
        "template": [
            "",
            " vim: fdm=marker:"
        ],
        "type": "rconfig"
    }
}
