{
    "ftdetect/*.vim": {"type": "fconfig"},
    "localcfg/plugin_*.vim": {
        "template": [
            "if SLoaded(expand(\"<sfile>\"))",
            "	finish",
            "endif",
            ""
        ],
        "type": "xconfig"
    },
    "localcfg/*.vim": {
        "template": [
            "if SLoaded(expand(\"<sfile>\"))",
            "	finish",
            "endif",
            ""
        ],
        "type": "lconfig"
    },
    "README.rst": {"type": "doc"},
    "dein.vim": {"type": "dein"},
    "vimrc": {"type": "rc"},
    "vimrc.d/*.vim": {
        "template": [
            "if SLoaded(expand(\"<sfile>\"))",
            "	finish",
            "endif",
            ""
        ],
        "type": "rconfig"
    }
}
