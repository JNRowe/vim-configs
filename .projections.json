{
    "extconfigs/*.vim": {
        "alternate": "external/{}",
        "template": [
            "if exists(\"g:loaded_xcfg_\" . expand(\"<sfile>:t:r:gs?[\\.-]?_?\"))",
            "	finish",
            "else",
            "	 execute(\"let g:loaded_xcfg_\" . expand(\"<sfile>:t:r:gs?[\\.-]?_?\") . \" = 1\")",
            "endif",
            ""
        ],
        "type": "config"
    },
    "README.rst": {"type": "doc"},
    "neobundle.vim": {"type": "neo"},
    "vimrc": {"type": "rc"}
}
