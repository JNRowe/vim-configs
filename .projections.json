{
    "*": {
        "make": "ninja -C ~ -j1"
    },
    "FAQ.rst": {
        "type": "faq"
    },
    "README.rst": {
        "type": "readme"
    },
    "after/ftplugin/*.rst": {
        "alternate": "after/ftplugin/{}.vim",
        "template": [
            "``after/ftplugin/{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            ""
        ],
        "type": "afconfig"
    },
    "after/ftplugin/*.vim": { "alternate": "after/ftplugin/{}.rst" },
    "after/syntax/*.rst": {
        "alternate": "after/syntax/{}.vim",
        "template": [
            "``after/syntax/{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            ""
        ],
        "type": "afsyntax"
    },
    "after/syntax/*.vim": { "alternate": "after/syntax/{}.rst" },
    "autoload/*.rst": {
        "alternate": "autoload/{}.vim",
        "template": [
            "``autoload/{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            ""
        ],
        "type": "autoload"
    },
    "autoload/*.vim": { "alternate": "autoload/{}.rst" },
    "autoload/plugins/*.rst": {
        "alternate": "autoload/plugins/{}.vim",
        "template": [
            "``autoload/plugins/{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            ""
        ],
        "type": "pautoload"
    },
    "autoload/plugins/*.vim": { "alternate": "autoload/plugins/{}.rst" },
    "build.py": {
        "alternate": "build.ninja",
        "type": "build"
    },
    "compiler/*.rst": {
        "alternate": "compiler/{}.vim",
        "template": [
            "``compiler/{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            ""
        ],
        "type": "compiler"
    },
    "compiler/*.vim": { "alternate": "compiler/{}.rst" },
    "dein.rst": {
        "alternate": "dein.vim",
        "type": "dein"
    },
    "dein.vim": { "alternate": "dein.rst" },
    "ftdetect/*.rst": {
        "alternate": "ftdetect/{}.vim",
        "template": [
            "``ftdetect/{}.vim``]|[\\hs1",
            "",
            "::",
            "",
            "    augroup jnrowe_ftdetect",
            "        autocmd BufRead,BufNewFile FIXME setfiletype FALLBACK FIXME",
            "    augroup END"
        ],
        "type": "fconfig"
    },
    "ftdetect/*.vim": { "alternate": "ftdetect/{}.rst" },
    "ftplugin/*.rst": {
        "alternate": "ftplugin/{}.vim",
        "template": [
            "``ftplugin/{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            ""
        ],
        "type": "ftplugin"
    },
    "ftplugin/*.vim": { "alternate": "ftplugin/{}.rst" },
    "localcfg/*.rst": {
        "alternate": "localcfg/{}.vim",
        "template": [
            "``localcfg/{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            ""
        ],
        "type": "lconfig"
    },
    "localcfg/*.vim": { "alternate": "localcfg/{}.rst" },
    "localcfg/plugin_*.rst": {
        "alternate": "localcfg/{}.vim",
        "template": [
            "``localcfg/plugin_{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            "",
            "        call MnemonicMap('{capitalize}', {'buffer': v:true})"
        ],
        "type": "xconfig"
    },
    "vimrc.d/*.rst": {
        "alternate": "vimrc.d/{}.vim",
        "template": [
            "``vimrc.d/{}.vim``]|[\\hs1",
            "",
            ".. include:: ../.includes/scriptencoding.rst",
            "",
            "::",
            ""
        ],
        "type": "rconfig"
    },
    "vimrc.d/*.vim": { "alternate": "vimrc.d/{}.rst" },
    "vimrc.rst": {
        "alternate": "vimrc",
        "type": "rc"
    },
    "vimrc": { "alternate": "vimrc.rst" }
}
