{
    "ftdetect/*.vim": {"type": "fconfig"},
    "localcfg/plugin_*.vim": {
        "type": "xconfig"
    },
    "localcfg/*.vim": {
        "type": "lconfig"
    },
    "*.rst": {
        "alternate": "{}.html",
        "type": "doc"
    },
    "dein.vim": {"type": "dein"},
    "vimrc": {"type": "rc"},
    "vimrc.d/*.vim": {
        "type": "rconfig"
    }
}
